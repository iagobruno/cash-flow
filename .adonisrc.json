{
  "typescript": true,
  "commands": [
    "@adonisjs/core/build/commands",
    "@adonisjs/repl/build/commands",
    "@adonisjs/lucid/build/commands",
    "@adonisjs/bouncer/build/commands"
  ],
  "exceptionHandlerNamespace": "App/Exceptions/GlobalHandler",
  "aliases": {
    "App": "app",
    "Config": "config",
    "Database": "database",
    "Contracts": "contracts"
  },
  "preloads": [
    "./start/routes",
    "./start/kernel",
    "./start/bouncer",
    {
      "file": "./start/validationRules",
      "environment": [
        "web"
      ]
    },
    {
      "file": "./start/events",
      "environment": [
        "console",
        "repl",
        "web"
      ]
    }
  ],
  "providers": [
    "./providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/auth",
    "@adonisjs/ally",
    "@adonisjs/session",
    "@adonisjs/bouncer"
  ],
  "aceProviders": [
    "@adonisjs/repl"
  ]
}
